<ListarVendedoresPage>:
    FloatLayout:

        ScrollView:
            pos_hint: {"right": 1, "top": 0.9}
            size_hint: 1, 1
            GridLayout:
                cols: 1
                size_hint_y: None
                height: self.minimum_height
                spacing: 3, 3
                row_default_height: "90dp"
                row_force_default: True
                id: lista
        FloatLayout:
            pos_hint: {"right": 1, "top": 0.12}
            size_hint: 1,0.12
            canvas:
                Color:
                    rgb: utils.get_color_from_hex("#006D7E")
                Rectangle:
                    size: self.size
                    pos: self.pos
            ImageButton:
                pos_hint: {"right": 1, "top": 0.9}
                size_hint: 0.33, 0.8
                source: "icones/voltar.png"
                on_release: on_release: app.mudar_tela("homepage")
