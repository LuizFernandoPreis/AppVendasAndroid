#: import utils kivy.utils

FloatLayout:
    canvas:
        Color:
            rgb: utils.get_color_from_hex("#001824")
        Rectangle:
            size: self.size
            pos: self.pos

    ImageButton:
        pos_hint: {"left": 1 , "top": 0.9}
        size_hint: 1,0.2
        source: "icones/hash.png"
        on_release: app.mudar_tela("homepage")
        id: foto_perfil
    ScreenManager:
        pos_hint: {"right": 1 , "top": 0.7}
        size_hint: 1,0.7
        id: screen_manager
        LoginPage:
            name: "loginpage"
            id: loginpage
        HomePage:
            name: "homepage"
            id: homepage
        AjustesPage:
            name: "ajustespage"
            id: ajustespage
        MudarFoto:
            name: "mudarfoto"
            id: mudarfoto
        AcompanharVendedor:
            name: "acompanharvendedor"
            id: acompanharvendedor
        ListarVendedoresPage:
            name: "listarvendedorespage"
            id: listarvendedorespage
        TodasVendasPage:
            name: "todasvendaspage"
            id: todasvendaspage
        AdicionarVendasPage:
            name: "adicionarvendaspage"
            id: adicionarvendaspage
        OutrosVendedoresPage:
            name: "outrosvendedorespage"
            id: outrosvendedorespage
